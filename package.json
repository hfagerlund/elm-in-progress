{
  "name": "elm-in-progress",
  "version": "0.0.0-semantically-released",
  "description": "An Elm app",
  "main": "src/Main.elm",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/github": "^5.0.5",
    "commitizen": "^2.10.1",
    "cz-customizable": "^5.2.0",
    "elm-test": "^0.18.12",
    "last-release-git": "0.0.3",
    "semantic-release": "^15.9.16"
  },
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfagerlund/elm-in-progress.git"
  },
  "author": "Heini Fagerlund",
  "license": "3-Clause BSD",
  "bugs": {
    "url": "https://github.com/hfagerlund/elm-in-progress/issues"
  },
  "homepage": "https://github.com/hfagerlund/elm-in-progress#readme",
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "getLastRelease": "last-release-git",
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ]
  }
}
